!commands add !educatedtravel $(eval var inp="$(query)",args=inp.split(" "),x=args[6],z=args[8],f=args[9];(f%=360)<0&&(f+=360);var o=256,d=90-(f-=180),x_1=x/8,z_1=z/8,r=d*(Math.PI/180),m_1=-1*Math.tan(r),a=1+m_1*m_1,b_1=-1*m_1*x_1+z_1,b=2*m_1*b_1,c_o=b_1*b_1-o*o,x_p=(-1*b+Math.sign(f)*Math.sqrt(b*b-4*a*c_o))/(2*a),z_p=m_1*x_p+b_1;x_p.toFixed(0) + ", "+ z_p.toFixed(0))