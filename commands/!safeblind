!addcom !safeblind $(eval var o,inp="$(query)",args=inp.split(" "),x=args[6],z=args[8],dist=Math.sqrt(x*x+z*z);o=dist<222?222:dist<250?dist:dist<480?250:dist<615?615:dist<645?dist:dist<832?645:dist<1005?1005:dist<1032?dist:1032;var t=Math.atan(z/x),x_p=Math.sign(x)*Math.abs(o*Math.cos(t)),z_p=Math.sign(z)*Math.abs(o*Math.sin(t));x_p.toFixed(0),z_p.toFixed(0);x_p.toFixed(0) + ", " + z_p.toFixed(0))