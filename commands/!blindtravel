!commands add !blindtravel $(eval var o,inp="$(query)",args=inp.split(" "),x=args[6],z=args[8],dist=Math.sqrt(x*x+z*z);o=dist<190?190:dist<310?dist:dist<460?310:dist<610?610:dist<670?dist:670;var t=Math.atan(z/x),x_p=Math.sign(x)*Math.abs(o*Math.cos(t)),z_p=Math.sign(z)*Math.abs(o*Math.sin(t));x_p.toFixed(0) + ", " + z_p.toFixed(0))