!commands add !blindtravel $(eval var o,inp="$(query)",args=inp.split(" "),x=args[6],z=args[8],dist=Math.sqrt(x*x+z*z);o=dist<190?190:dist<290?dist:dist<480?290:dist<594?594:dist<686?dist:dist<832?686:dist<970?970:dist<1060?dist:1060;var t=Math.atan(z/x),x_p=Math.sign(x)*Math.abs(o*Math.cos(t)),z_p=Math.sign(z)*Math.abs(o*Math.sin(t));x_p.toFixed(0),z_p.toFixed(0);x_p.toFixed(0) + ", " + z_p.toFixed(0))